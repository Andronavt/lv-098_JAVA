<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="parserResultService" class="tc.lv.service.implementation.ParserResultServiceImpl" />
	<bean id="geoIpAdvice" class="tc.lv.utils.GeoIpAdvice" />

	<bean id="parserResultServiceProxy" class="org.springframework.aop.framework.ProxyFactoryBean">

		<property name="target" ref="parserResultService" />

		<property name="interceptorNames">
			<list>
				<value>geoIpAdvice</value>
			</list>
		</property>
	</bean>

</beans>